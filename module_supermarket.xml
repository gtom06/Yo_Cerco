  <patternset id="excluded.from.module.supermarket">
    <patternset refid="ignored.files"/>
  </patternset>
  
  <patternset id="excluded.from.compilation.supermarket">
    <patternset refid="excluded.from.module.supermarket"/>
  </patternset>
  
  <path id="supermarket.module.sourcepath">
    <dirset dir="${module.supermarket.basedir}">
      <include name="src"/>
    </dirset>
  </path>
  
  
  <target name="compile.module.supermarket" depends="compile.module.supermarket.production,compile.module.supermarket.tests" description="Compile module Supermarket"/>
  
  <target name="compile.module.supermarket.production" depends="register.custom.compilers" description="Compile module Supermarket; production classes">
    <mkdir dir="${supermarket.output.dir}"/>
    <javac2 destdir="${supermarket.output.dir}" debug="${compiler.debug}" nowarn="${compiler.generate.no.warnings}" memorymaximumsize="${compiler.max.memory}" fork="true" executable="${module.jdk.bin.supermarket}/javac">
      <compilerarg line="${compiler.args.supermarket}"/>
      <bootclasspath refid="supermarket.module.bootclasspath"/>
      <classpath refid="supermarket.module.production.classpath"/>
      <src refid="supermarket.module.sourcepath"/>
      <patternset refid="excluded.from.compilation.supermarket"/>
    </javac2>
    
    <copy todir="${supermarket.output.dir}">
      <fileset dir="${module.supermarket.basedir}/src">
        <patternset refid="compiler.resources"/>
        <type type="file"/>
      </fileset>
    </copy>
  </target>
  
  <target name="compile.module.supermarket.tests" depends="register.custom.compilers,compile.module.supermarket.production" description="compile module Supermarket; test classes" unless="skip.tests"/>
  
  <target name="clean.module.supermarket" description="cleanup module">
    <delete dir="${supermarket.output.dir}"/>
    <delete dir="${supermarket.testoutput.dir}"/>
  </target>
</project>