                    <Image url="@../../images/user_interface/icons8-home-48.png" />
                </image>
            </ImageView>
        </AnchorPane>
        <AnchorPane layoutY="50.0" maxHeight="650.0" maxWidth="1000.0" minHeight="313.0" minWidth="1000.0" prefHeight="650.0" prefWidth="1000.0" style="-fx-background-color: #2c2c2c; -fx-border-color: #2c2c2c; -fx-border-width: 5;">
