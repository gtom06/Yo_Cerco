                <TableView onMouseClicked="#onOrderClicked" fx:id="orderTableView" layoutX="21.0" layoutY="28.0" prefHeight="576.0" prefWidth="962.0" />
                <Text fx:id="totalOrdersText" layoutX="21.0" layoutY="630.0" fill="#12d39d" stroke="#0a7557" strokeLineCap="BUTT" strokeLineJoin="ROUND" strokeType="OUTSIDE" strokeWidth="3.0">
