      xmlns:fx="http://javafx.com/fxml/1" fx:controller="view.view1.OrdersAdmin">
    <children>
        <AnchorPane maxHeight="50.0" maxWidth="1000.0" minHeight="50.0" minWidth="1000.0" prefHeight="50.0" prefWidth="1000.0" style="-fx-background-color: #2c2c2c;">
            <Text fx:id="textHi" fill="WHITE" layoutX="923.0" layoutY="32.0" >
                <font>
                    <Font name="Tahoma Bold" size="18.0" />
                </font>
            </Text>
            <ImageView fx:id="homepageImageView1OA" fitHeight="40.0" fitWidth="40.0" layoutX="26.0" layoutY="5.0" onMouseClicked="#onHomepageImageClick" pickOnBounds="true" preserveRatio="true">
